<Window x:Class="Stat_talon.Print_write_on_reception"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stat_talon"
        mc:Ignorable="d"
           Title="Печать запись на прием" Height="481.902" Width="917.712" Loaded="Window_Loaded"  WindowStartupLocation="CenterScreen" FontSize="24" FontFamily="Times New Roman" Background="#FF011A3E" ResizeMode="NoResize">
    <Grid>
        <Grid Name="grid_day" d:IsHidden="True">
            <DatePicker CalendarStyle="{StaticResource TouchCalendarStyle}"  Name="datepicker_date" HorizontalAlignment="Left" Margin="311,96,0,0" VerticalAlignment="Top" Width="273" DisplayDate="2018-05-14" Text="Datetime.Noew"/>
            <Button Name="button_print" Content="Печать" HorizontalAlignment="Left" Margin="256,214,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click"/>
            <Button Name="button_close" Content="Закрыть" HorizontalAlignment="Left" Margin="490,214,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click_1"/>
            <Label Name="label__1" Content="Распечатать?" HorizontalAlignment="Left" Margin="340,137,0,0" VerticalAlignment="Top" Foreground="White" FontSize="36"/>
            <Button Name="button_yes" Content="Да" HorizontalAlignment="Left" Margin="256,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_yes_Click"/>
            <Button Name="button_no" Content="Нет" HorizontalAlignment="Left" Margin="490,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_no_Click"/>

        </Grid>
        <Grid Name="grid_week" d:IsHidden="True">
            <Button Name="button___" Content="Печать" HorizontalAlignment="Left" Margin="229,249,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click"/>
            <Button Name="button____" Content="Закрыть" HorizontalAlignment="Left" Margin="463,249,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click_1"/>
            <Label Name="label_begin_end" Content="Label" Margin="229,131,257,270" Height="52" Width="426" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="28"/>
            <Label Name="label___" Content="Распечатать?" HorizontalAlignment="Left" Margin="340,193,0,0" VerticalAlignment="Top" Foreground="White" FontSize="36"/>
            <Button Name="button__yes" Content="Да" HorizontalAlignment="Left" Margin="230,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_yes_Click"/>
            <Button Name="button__no" Content="Нет" HorizontalAlignment="Left" Margin="464,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_no_Click"/>

        </Grid>
        <Grid Name="grid_month">
            <Button x:Name="button_print_Copy" Content="Печать" HorizontalAlignment="Left" Margin="256,214,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click"/>
            <Button x:Name="button_close_Copy" Content="Закрыть" HorizontalAlignment="Left" Margin="490,214,0,0" VerticalAlignment="Top" Width="191" Height="59" Background="White" BorderBrush="Black" Click="Button_Click_1"/>
            <Label x:Name="label__1_Copy" Content="Распечатать?" HorizontalAlignment="Left" Margin="340,137,0,0" VerticalAlignment="Top" Foreground="White" FontSize="36"/>
            <Button x:Name="button_yes_Copy" Content="Да" HorizontalAlignment="Left" Margin="256,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_yes_Click"/>
            <Button x:Name="button_no_Copy" Content="Нет" HorizontalAlignment="Left" Margin="490,313,0,0" VerticalAlignment="Top" Width="191" Height="59" BorderBrush="White" Background="White" Click="button_no_Click"/>
            <Label x:Name="label_begin_end_Copy" Content="Label" Margin="244,101,242,300" Height="52" Width="426" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="28"/>
        </Grid>
        <Button Content="Выбрать врача" BorderThickness="3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="169" Height="39" Margin="10,10,0,0" Foreground="White" Background="{x:Null}" BorderBrush="White" Click="Button_Click_2"/>
    </Grid>

</Window>
